{
  "name": "bvh_web",
  "version": "0.0.1",
  "description": "Django, React, Material-UI, Webpack (with Hot Module Replacement) and Babel.",
  "main": "src/static/index.jsx",
  "keywords": [
    "react",
    "webpack",
    "babel",
    "less",
    "hmr",
    "starter",
    "boilerplate",
    "material-ui"
  ],
  "author": "Kozlitin Valeriy <valera_k2000@inbox.ru>",
  "license": "MIT",
  "scripts": {
    "prod-server": "node react-node-express/express.js",
    "build": "npm run clean-dist && webpack -p --progress --config=webpack/prod.config.js",
    "build-analysis": "npm run clean-dist && webpack -p --progress --config=webpack/analyzer.config.js",
    "build-dev": "webpack-dev-server --progress --client-log-level info --config=webpack/dev.config.js --watch",
    "dev": "webpack --progress --display-error-details --config=webpack/dev.config.js --watch",
    "clean-dist": "rimraf -f -rmdir dist",
    "lint": "npm-run-all lint-js lint-less",
    "lint-js": "jshint",
    "lint-less": "lesshint ./src/assets/less",
    "start": "npm run dev",
    "start-prod": "webpack --progress -p --config=webpack/prod.config.js",
    "start-node": "npm run build-dev",
    "start-prod-node": "npm-run-all build prod-server",
    "start-analysis": "npm run build-analysis",
    "test-bundle": "bundlesize",
    "test": "jest --watch --coverage --config=jest/jest.json",
    "test-ci": "jest --ci --coverage --config=jest/jest.json && codecov -t 75b11d17-22d7-4b30-81b1-4c39fa2f7582",
    "check": "npm-check",
    "cost": "cost-of-modules --no-install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valerak2000/bvh_web.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/googlemaps": "^3.30.12",
    "@types/markerclustererplus": "^2.1.33",
    "@types/prop-types": "^15.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "bundlesize": "^0.17.0",
    "compression": "^1.7.3",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.4.0",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "extract-css-chunks-webpack-plugin": "^3.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "imagemin": "^6.0.0",
    "img-loader": "^3.0.0",
    "jest": "^23.5.0",
    "jest-html-reporter": "^2.4.2",
    "jsdom-global": "^3.0.2",
    "jshint": "^2.9.6",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lesshint": "^5.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.3.5",
    "redux-devtools-extension": "^2.13.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "size-plugin": "^1.0.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-dev-server": "^3.1.6",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "axios": "^0.18.0",
    "chartist": "^0.11.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.7.5",
    "linq": "^3.1.0",
    "lodash": "^4.17.10",
    "perfect-scrollbar": "^1.4.0",
    "react": "^16.4.2",
    "react-chartist": "^0.13.1",
    "react-dom": "^16.4.2",
    "react-google-maps": "latest",
    "react-redux": "^5.0.7",
    "react-redux-thunk-store": "^0.0.7",
    "react-router-dom": "^4.3.1",
    "react-table": "^6.8.6",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0",
    "redux-toolbelt": "^2.1.5",
    "redux-toolbelt-thunk": "^2.0.4",
    "url-join": "^4.0.0",
    "uuid": "^3.3.2",
    "material-ui": "0.20.1",
    "font-awesome": "4.7.0",
    "font-awesome-webpack": "0.0.5-beta.2",
    "node-sass": "4.9.3",
    "npm": "^6.4.1",
    "react-favicon": "0.0.14",
    "react-pdf": "4.0.0-beta.3"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/tests/__coverage__/",
    "collectCoverage": true
  },
  "bundlesize": [
    {
      "path": "./src/static_dist/*.*",
      "compression": "gzip",
      "maxSize": "100 kB"
    },
    {
      "path": "./src/static_dist/*.*",
      "compression": "none",
      "maxSize": "300 kB"
    }
  ]
}
