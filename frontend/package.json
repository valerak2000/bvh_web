{
  "name": "bvh_web",
  "version": "0.0.1",
  "description": "Django, React 16, Material-UI, Webpack 4 (with Hot Module Replacement) and Babel 7.",
  "main": "src/index.jsx",
  "keywords": [
    "react 16",
    "webpack 4",
    "babel",
    "less",
    "redux",
    "react-redux",
    "hot-reload",
    "redux-devtools-extension",
    "hmr",
    "starter",
    "boilerplate",
    "material-ui"
  ],
  "author": "Kozlitin Valeriy <valera_k2000@inbox.ru>",
  "license": "MIT",
  "scripts": {
    "prod-server": "node react-node-express/express.js",
    "build": "npm run clean-dist && webpack -p --progress --config=webpack/prod.config.js",
    "build-analysis": "npm run clean-dist && webpack -p --progress --config=webpack/analyzer.config.js",
    "build-dev": "npm run clean-dist && webpack-dev-server --progress --client-log-level info --config=webpack/dev.config.js --watch",
    "dev": "npm run clean-dist && webpack --progress --display-error-details --config=webpack/dev.config.js --watch",
    "clean-dist": "rimraf -f -rmdir ../static/bundles",
    "lint": "npm-run-all lint-js lint-less",
    "lint-js": "jshint",
    "lint-less": "lesshint ./src/bvh_web/assets/less",
    "start": "npm run dev",
    "start-prod": "webpack --progress --display-error-details -p --config=webpack/prod.config.js",
    "start-node": "npm run build-dev",
    "start-prod-node": "npm-run-all build prod-server",
    "start-analysis": "npm run build-analysis",
    "test-bundle": "bundlesize",
    "test": "jest --watch --coverage --config=jest/jest.json",
    "test-ci": "jest --ci --coverage --config=jest/jest.json && codecov -t 75b11d17-22d7-4b30-81b1-4c39fa2f7582",
    "check": "npm-check",
    "cost": "cost-of-modules --no-install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valerak2000/bvh_web.git"
  },
  "bugs": {
    "url": "https://github.com/valerak2000/bvh_web/issues"
  },
