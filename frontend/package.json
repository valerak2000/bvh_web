  "scripts": {
    "prod-server": "node react-node-express/express.js",
    "build": "webpack -p --progress --display-error-details --colors --config=webpack/prod.config.js",
    "build-analysis": "webpack -p --progress --config=webpack/analyzer.config.js",
    "build-dev": "webpack-dev-server -p --progress --display-error-details --client-log-level info --colors --config=webpack/dev.config.js --watch",
    "dev": "webpack --progress --display-error-details --colors --config=webpack/dev.config.js --watch",
    "dev-nowatch": "webpack --progress --display-error-details --colors --config=webpack/dev.config.js",
    "lint": "npm-run-all lint-js lint-less",
    "lint-js": "jshint",
    "lint-less": "lesshint ./src/bvh_web/assets/less",
    "start": "npm run dev",
    "start-nowatch": "npm run dev-nowatch",
    "start-prod": "npm run build",
    "start-node": "npm run build-dev",
    "start-prod-node": "npm-run-all build prod-server",
    "start-analysis": "npm run build-analysis",
    "test-bundle": "bundlesize",
    "test": "jest --watch --coverage --config=jest/jest.json",
    "test-ci": "jest --ci --coverage --config=jest/jest.json && codecov -t 75b11d17-22d7-4b30-81b1-4c39fa2f7582",
    "check": "npm-check",
    "cost": "cost-of-modules --no-install"
  },
